    "直接播放直链视频请求头": "authority$ku.peizq.online#Referer$https://play.peizq.online",
    
    //项目信息jsoup与正则截取写法切换，1为jsoup写法(默认)，0为正则截取写法
    "详情是否Jsoup写法":"0",
    //类型数据，截取前缀&&截取后缀
    "类型详情": "类型：</span>&&</p>",
    //年代数据，截取前缀&&截取后缀
    "年代详情": "年代：</span>&&</p>",
    //地区数据，截取前缀&&截取后缀
    "地区详情": "地区：</span>&&</p>",
    //演员数据，截取前缀&&截取后缀
