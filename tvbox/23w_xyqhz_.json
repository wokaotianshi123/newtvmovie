    //简介内容，截取前缀&&截取后缀
    "简介详情": "class=\"desc_txt cf ec-palytcji col1c\"&&</span>",
    
    //线路截取区域，如果不需要请把tab_title或tab_arr_rule置空或者全部不要填。
    //线路截取数组
    "线路列表数组规则": "#tag&&.swiper-wrapper&&a",
    //线路标题，截取前缀&&截取后缀
    "线路标题": "a&&alt",
    
    //列表数组截取，必须
    "播放列表数组规则": "body&&.content_playlist",
    //集数数组截取，必须
    "选集列表数组规则": "li",
